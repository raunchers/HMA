create table visitors(
    VISIT_ID int(20) not null PRIMARY KEY AUTO_INCREMENT,
    VISIT_FIRST_NAME varchar(30) not null,
    VISIT_LAST_NAME varchar(30) not null,
    VISIT_ZIP_CODE varchar(10) not null,
    VISIT_ETHNICITY varchar(20) not null,
    VISIT_HEARD varchar(200) not null,
    VISIT_DATE date not null
);


